{
    "documents": {
        "word_to_pdf": {
            "priority": 1,
            "status": "DONE",
            "description": "Convert Word document (DOCX) to PDF format - Client-side",
            "clientSide": true,
            "dependencies": ["mammoth", "jspdf", "html2canvas"],
            "input": {
                "type": "buffer",
                "format": "docx",
                "required": ["buffer"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Limited formatting preservation. Converts DOCX to HTML via Mammoth, then renders to PDF via jsPDF"
        },
        "pdf_to_word": {
            "priority": 2,
            "status": "ON_HOLD",
            "description": "Extract text from PDF to create a basic Word document - Client-side",
            "clientSide": true,
            "dependencies": ["pdf-lib", "docx"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer"]
            },
            "output": {
                "type": "buffer",
                "format": "docx",
                "mimeType": "application/vnd.openxmlformats-officedocument.wordprocessingml.document"
            },
            "notes": "Text extraction only, no formatting preservation. Creates new DOCX with extracted text"
        },
        "pdf_merge": {
            "priority": 3,
            "status": "DONE",
            "description": "Merge multiple PDF documents into a single PDF",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer[]",
                "format": "pdf",
                "required": ["buffers"],
                "minItems": 2
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Maintains order of input files. Uses pdf-lib for browser-based merging"
        },
        "pdf_split": {
            "priority": 4,
            "status": "DONE",
            "description": "Split PDF document into individual pages or ranges",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer"],
                "optional": ["pages"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Can extract specific pages or all pages as separate files. Returns single page PDF"
        },
        "pdf_add_text_watermark": {
            "priority": 5,
            "status": "DONE",
            "description": "Add text watermark to PDF document",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer", "text"],
                "optional": ["fontSize", "opacity", "rotation", "color", "position"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Customizable text, font, color, opacity, and position using pdf-lib"
        },
        "pdf_add_image_watermark": {
            "priority": 6,
            "status": "DONE",
            "description": "Add image watermark to PDF document",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer", "watermarkBuffer"],
                "optional": ["opacity", "rotation", "scale", "position"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Supports PNG, JPEG, WebP watermarks. Embeds image into PDF pages"
        },
        "pdf_metadata_updater": {
            "priority": 7,
            "status": "TESTING_LEFT",
            "description": "Updates metadata of a PDF document",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Removes author, title, subject, keywords, creator, producer, creation date, modification date"
        },
        "pdf_compress": {
            "priority": 8,
            "status": "ON_HOLD",
            "description": "Compress PDF document to reduce file size",
            "clientSide": true,
            "dependencies": ["pdf-lib"],
            "input": {
                "type": "buffer",
                "format": "pdf",
                "required": ["buffer"],
                "optional": ["quality", "compressImages"]
            },
            "output": {
                "type": "buffer",
                "format": "pdf",
                "mimeType": "application/pdf"
            },
            "notes": "Basic compression. Removes unused objects and streams. Image compression limited in browser"
        }
    },
