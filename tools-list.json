            },
            "output": {
                "type": "buffer",
                "format": "jpeg|png|webp",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Browser Canvas API. Supports JPEG, PNG, WebP. AVIF/TIFF require libraries like image-conversion"
        },
        "image_compress": {
            "priority": 2,
            "status": "NOT STARTED",
            "description": "Compress image to reduce file size",
            "clientSide": true,
            "dependencies": ["browser-image-compression"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"],
                "optional": ["quality", "format", "maxSizeMB", "maxWidthOrHeight"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Uses browser-image-compression library. Reduces quality while maintaining reasonable visual fidelity"
        },
        "image_resize": {
            "priority": 3,
            "status": "NOT STARTED",
            "description": "Resize image to specified dimensions",
            "clientSide": true,
            "dependencies": ["native-canvas", "pica"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer", "width", "height"],
                "optional": ["maintainAspectRatio", "fit", "background"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Uses Canvas API with Pica for quality resize. fit: cover, contain, fill, inside, outside"
        },
        "image_crop": {
            "priority": 4,
            "status": "NOT STARTED",
            "description": "Crop image to specified region",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer", "left", "top", "width", "height"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Coordinates are in pixels from top-left corner. Uses Canvas API"
        },
        "image_rotate": {
            "priority": 5,
            "status": "NOT STARTED",
            "description": "Rotate image by specified angle",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer", "angle"],
                "optional": ["background", "expand"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Angle in degrees, positive is clockwise. Uses Canvas API"
        },
        "image_flip": {
            "priority": 6,
            "status": "NOT STARTED",
            "description": "Flip image horizontally or vertically",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"],
                "optional": ["direction"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "direction: 'horizontal' | 'vertical' | 'both'. Uses Canvas API"
        },
        "image_strip_exif": {
            "priority": 7,
            "status": "NOT STARTED",
            "description": "Remove EXIF metadata from image",
            "clientSide": true,
            "dependencies": ["piexifjs"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Removes EXIF data from JPEG. PNG/WebP re-encoding naturally strips metadata"
        },
        "image_dominant_color": {
            "priority": 8,
            "status": "NOT STARTED",
            "description": "Extract dominant color from image",
            "clientSide": true,
            "dependencies": ["node-vibrant"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"],
                "optional": ["colorCount", "quality"]
            },
            "output": {
                "type": "object",
                "format": "hex|rgb",
                "mimeType": null
            },
            "notes": "Returns hex color code and palette. Uses node-vibrant which works in browser"
        }
    },
