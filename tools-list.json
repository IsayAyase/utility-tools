        "image_transform": {
            "priority": 4,
            "status": "TESTING_LEFT",
            "description": "Apply crop, rotate, and/or flip transformations to image",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"],
                "optional": ["crop", "rotate", "flip"]
            },
            "output": {
                "type": "buffer",
                "format": "png",
                "mimeType": "image/png"
            },
            "notes": "Unified function for crop, rotate, and flip. Apply multiple transformations in sequence: crop → rotate → flip. Uses Canvas API"
        },
        "image_strip_exif": {
            "priority": 5,
            "status": "TESTING_LEFT",
            "description": "Remove EXIF metadata from image",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"]
            },
            "output": {
                "type": "buffer",
                "format": "original",
                "mimeType": "image/jpeg|image/png|image/webp"
            },
            "notes": "Removes EXIF by re-encoding image. PNG/WebP naturally strip metadata."
        },
        "image_dominant_color": {
            "priority": 6,
            "status": "TESTING_LEFT",
            "description": "Extract dominant color from image",
            "clientSide": true,
            "dependencies": ["native-canvas"],
            "input": {
                "type": "buffer",
                "format": "any",
                "required": ["buffer"]
            },
            "output": {
                "type": "object",
                "format": "hex",
                "mimeType": null
            },
            "notes": "Analyzes image pixels to find most frequent color. Returns hex code and palette."
        }
